# Gemini API Key (from https://aistudio.google.com/apikey)
GEMINI_API_KEY=your_gemini_api_key

# Database settings
DB_ADDR=localhost:5432
DB_USER=postgres
DB_PASS=your_password

# NATS settings
NATS__URL=nats://localhost:4222
NATS__NUM_OF_SUMMARIZER_CONSUMER_INSTANCES=3

# Service settings
SERVICE_PORT=8004

# Optional: Google Cloud Storage settings
# Base64-encoded service account JSON (for GCS access)
# Generate with: cat service-account.json | base64 -w 0
GCP_PROJECT_ID=
GCP_CREDENTIALS_BASE64=
